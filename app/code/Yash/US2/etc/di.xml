<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="Magento\Framework\Pricing\Render\PriceBox" >
        <plugin name="my_price_plugin" type="Yash\US2\Plugin\PricePlugin" />
    </type>

    <type name="Magento\Theme\Block\Html\Footer" >
        <plugin name="my_copyright_plugin" type="Yash\US2\Plugin\CopyRightPlugin" />
    </type>

    <type name="Magento\Theme\Block\Html\Header" >
        <plugin name="my_message_plugin" type="Yash\US2\Plugin\WelcomeMessagePlugin" />
    </type>

    <type name="Magento\Theme\Block\Html\Breadcrumbs" >
        <plugin name="my_breadcrumbs_plugin" type="Yash\US2\Plugin\BreadcrumbsPlugin" />
    </type>

    <type name="Magento\Catalog\Model\Product">
        <plugin name="my_product_name_plugin" type="Yash\US2\Plugin\ProductNamePlugin"/>
    </type>

    <type name="Magento\Catalog\Block\Product\ListProduct">
        <plugin name="my_product_logo_plugin" type="Yash\US2\Plugin\AddLogoPlugin"/>
    </type>
</config>